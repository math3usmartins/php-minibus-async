{
  "license": "MIT",
  "type": "library",
  "name": "math3usmartins/php-minibus-transport",
  "autoload": {
    "psr-4": {
      "MiniBus\\Transport\\": "src",
      "MiniBus\\Test\\Transport\\": "test"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:math3usmartins/php-minibus.git"
