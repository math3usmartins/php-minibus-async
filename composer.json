  "require": {
    "php": "^7.0",
    "math3usmartins/php-minibus": "^2.0"
  },
  "suggest": {
    "ext-json": "*",
    "ext-pcntl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "phpunit/php-code-coverage": "^5.3",
    "friendsofphp/php-cs-fixer": "^2.19",
    "phpstan/phpstan": "^0.9.3"
  }
}
